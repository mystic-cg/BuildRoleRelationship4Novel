    "source": [
     "{\n",
     "  \"nbformat\": 4,\n",
     "  \"nbformat_minor\": 0,\n",
     "  \"metadata\": {\n",
     "    \"colab\": {\n",
     "      \"name\": \"Stanza-Beginners-Guide.ipynb\",\n",
     "      \"provenance\": [],\n",
     "      \"collapsed_sections\": [],\n",
     "      \"toc_visible\": true\n",
     "    },\n",
     "    \"kernelspec\": {\n",
